{
  "name": "Github contributions colorful",
  "description": "customize your lovely color for the Github contributions",
  "manifest_version": 3,
  "version": "1.1",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://github.com/*"],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_popup": "popup/index.html"
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
